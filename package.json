  "productName": "gyro",
  "author": "Gal Saada <gal@novos.gg>",
  "private": true,
  "scripts": {
    "dev": "cross-env QENV=development quasar dev",
    "dev-pwa": "env QENV=development quasar dev -m pwa",
    "test-dev": "env QENV=test quasar dev",
    "staging-dev": "env QENV=staging quasar dev",
    "build": "env QENV=prod quasar build -m pwa",
    "build-test": "env QENV=test quasar build -m pwa",
    "build-staging": "env QENV=staging quasar build -m pwa",
    "build-deploy-test": "npm run build-test && sh ./scripts/deploy.sh test",
    "build-deploy-staging": "npm run build-staging && sh ./scripts/deploy.sh staging",
    "build-deploy-prod": "npm run build && sh ./scripts/deploy.sh prod",
    "build-with-sourcemaps-stg": "bash ./scripts/build-with-sourcemaps.sh staging",
    "build-with-sourcemaps-prod": "bash ./scripts/build-with-sourcemaps.sh prod",
    "deploy-with-sentry-stg": "bash ./scripts/deploy-with-sentry.sh staging",
    "deploy-with-sentry-prod": "bash ./scripts/deploy-with-sentry.sh prod",
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"No test specified\" && exit 0",
    "release": "standard-version",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "npm run build-storybook && bash ./scripts/deploy_storybook.sh"
  },
